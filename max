# include <stdio.h>

int max (int x,int y,int z);

int main (void)
{
	int nu1,nu2,nu3;
	
	printf("Enter 3 numbers :");
	scanf("%d %d %d",&nu1,&nu2,&nu3);
	
	printf("Ma>< is %d",max(nu1,nu2,nu3));
	
	return 0;
}

int max (int x,int y,int z)
{
	int max = x;
	
	if(y>max)
	{
		max=y;
	}
	if(z>max)
	{
		max=z;
	}
		return max;

}
